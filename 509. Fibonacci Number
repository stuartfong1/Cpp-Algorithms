// Dynamic Programming
class Solution {
public:
    int fib(int N) {
        vector<int> dp = {0, 1, 1, 2};
        if(N >= dp.size())
        {
            for(int i = 4; i <= N; ++i)
            {
                dp.push_back(dp[i-1]+dp[i-2]);
            }
        }
        return(dp[N]);
    }
};
